{
	"name": "letterboxd-discord-bot",
	"version": "0.1.0",
	"description": "Letterbox Discord Bot",
	"main": "bot.js",
	"repository": "https://github.com/caseypugh/letterboxd-discord-bot",
	"author": "Casey Pugh",
	"license": "MIT",
	"engines": {
		"node": "16"
	},
	"scripts": {
		"start": "yarn db:migrate && yarn db:generate && NODE_PATH=./ ts-node src/app.ts",
		"dev": "NODE_PATH=./ nodemon --watch \"src/**/*\" --ext ts --exec \"ts-node src/app.ts\"",
		"redis": "sudo service redis-server start",
		"deploy-commands": "NODE_PATH=./ ts-node src/tools/deploy-commands.ts",
		"deploy-heroku": "git push heroku main && heroku logs -t",
		"test": "ts-node test.ts",
		"db:migrate": "prisma migrate dev",
		"db:generate": "prisma generate"
	},
	"dependencies": {
		"@discordjs/builders": "^0.13.0",
		"@discordjs/rest": "^0.4.1",
		"@keyv/redis": "^2.3.4",
		"@prisma/client": "4.7.1",
		"cron": "^1.8.2",
		"discord-api-types": "^0.32.0",
		"discord.js": "^13.6.0",
		"dotenv": "^8.2.0",
		"javascript-time-ago": "^2.3.13",
		"letterboxd": "^3.0.0",
		"node-fetch": "2",
		"promise-delay-ts": "^1.0.1",
		"request": "^2.88.2",
		"request-promise-native": "^1.0.9",
		"rss-parser": "^3.12.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.6.3"
	},
	"devDependencies": {
		"@types/cron": "^1.7.3",
		"@types/node": "^18.11.17",
		"@types/node-fetch": "^2.6.1",
		"nodemon": "^2.0.16",
		"prisma": "^4.7.1"
	}
}
